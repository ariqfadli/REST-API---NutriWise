generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model child {
  id                 String       @id @default(cuid())
  child_name         String       @db.VarChar(45)
  date_of_birth      DateTime?    
  height             Float
  weight             Float
  gender             String       @db.VarChar(45)
  head_circumference Float
}

